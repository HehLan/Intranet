            function getBracket_75()
        {
            // Initial data if is not encoded in the database
            var result_75 = null;

            // Getting the bracket - the group_number must be dynamic
            $.ajax(
                {
                    url: '../common/bracket_get.php?id_tournoi=2&type=1&number=75',
                    type: 'GET',
                    dataType: 'text',
                    success: function (text_75, status)
                    {
                        
                        //if(text_75.contains("error"))
                        //{
                            //alert(text_75);
                            //result_75 = null;
                        //}
                        //else
                        //{ 
                            result_75 = JSON.parse(text_75);
                        //}
                    },
                    error: function (resultat, statut, erreur)
                    {

                    },
                    complete: function (resultat, statut)
                    {

                    },
                    async: false
                }
            );
            return result_75;
        }
