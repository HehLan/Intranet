    // Function when document is ready
    $(function ()
    {
                  
            var groupData_75 = null;
                  
            var groupData_76 = null;
                  
            var groupData_81 = null;
        
          
            groupData_75 = getBracket_75();
          
            groupData_76 = getBracket_76();
          
            groupData_81 = getBracket_81();
          
            
                    $("#nav-tab-75").click(function ()
                {
                    var groupData_75 = null;
                    groupData_75 = getBracket_75();
                    var editor_75 = $("#editor-75");
                    var view_75 = $("#view-75");
                    
                    editor_75.empty();
                    editor_75.group(
                        { 
                            init: groupData_75,
                            save: saveBracket_75
                        }
                    );

                    view_75.empty();
                    view_75.group(
                        {
                            init: groupData_75
                        }
                    );
                }
            ); 
