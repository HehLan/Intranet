            function saveBracket_75(state)
        {
            // Write your storage code here, now just display JSON above
            $("#state-75").text(JSON.stringify(state, undefined, 2));
            // Reconstruct read-only version by initializing it with received state
            $("#view-75").empty().group({
                init: state
            });  
            $.ajax(
                {
                    url: 'modules/bracket_save.php',
                    type: 'POST',
                    data: "json=" + $("#state-75").text() + "&id_tournoi=2&type=1&number=75", 
                    dataType: 'text'
                }
            );      
        };   
            function saveBracket_76(state)
        {
            // Write your storage code here, now just display JSON above
            $("#state-76").text(JSON.stringify(state, undefined, 2));
            // Reconstruct read-only version by initializing it with received state
            $("#view-76").empty().group({
                init: state
            });  
            $.ajax(
                {
