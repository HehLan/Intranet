            function getBracket_72()
        {
            // Initial data if is not encoded in the database
            var result_72 = null;

            // Getting the bracket - the group_number must be dynamic
            $.ajax(
                {
                    url: '../common/bracket_get.php?id_tournoi=5&type=1&number=72',
                    type: 'GET',
                    dataType: 'text',
                    success: function (text_72, status)
                    {
                        
                        //if(text_72.contains("error"))
                        //{
                            //alert(text_72);
                            //result_72 = null;
                        //}
                        //else
                        //{ 
                            result_72 = JSON.parse(text_72);
                        //}
                    },
                    error: function (resultat, statut, erreur)
                    {

                    },
                    complete: function (resultat, statut)
                    {

                    },
                    async: false
                }
            );
            return result_72;
        }
