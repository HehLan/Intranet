                    $("#nav-tab-72").click(function test_72 ()
                {
                    var groupData_72 = null;
                    groupData_72 = getBracket_72();
                    var editor_72 = $("#editor-72");
                    var view_72 = $("#view-72");
                    
                    editor_72.empty();
                    editor_72.group(
                        { 
                            init: groupData_72,
                            save: saveBracket_72
                        }
                    );

                    view_72.empty();
                    view_72.group(
