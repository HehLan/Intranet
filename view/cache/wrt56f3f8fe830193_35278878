            function saveBracket_72(state)
        {
            // Write your storage code here, now just display JSON above
            $("#state-72").text(JSON.stringify(state, undefined, 2));
            // Reconstruct read-only version by initializing it with received state
            $("#view-72").empty().group({
                init: state
            });  
            $.ajax(
                {
                    url: 'modules/bracket_save.php',
                    type: 'POST',
                    data: "json=" + $("#state-72").text() + "&id_tournoi=5&type=1&number=72", 
                    dataType: 'text'
                }
            );      
        };   
            function saveBracket_73(state)
        {
